<%= form_for [:admin, @seo] do |f| %>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </div>
  <div class="field">
    <%= f.label :keywords %><br />
    <%= f.text_area :keywords %>
  </div>
  <div class="field">
    <%= f.label :seo_obj %><br />
    <%= select_tag "item_id", options_from_collection_for_select(Item.all, "id", "title"), :prompt => "Select item" %>
    <%= select_tag "category_id", options_from_collection_for_select(Category.all, "id", "title"), :prompt => "Select category" %>
    <%= select_tag "welcome_id", options_for_select([['welcome','1']]), :prompt => "Select welcome" %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>