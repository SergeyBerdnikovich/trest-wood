<!DOCTYPE html>
<html>
<head>
  <title>TrestWood</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="span12">
      <div class="upper_zone">
        <%= render "layouts/zone/upper_zone" %>
      </div>
    </div>
    <div class="span12">
      <div class="menu_zone">
        <%= render "layouts/zone/menu_zone" %>
      </div>
    </div>
    <div class="span12">
      <p class="notice"><%= notice %></p>
      <div class="content_zone">
        <%= yield %>
        <br><br><br><br><br><br>
      </div>
    </div>
  </div>
</body>
</html>
